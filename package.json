{
  "name": "docsy",
  "version": "1.0.0-3",
  "description": "A syntax for Component based documents",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/docsy#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/docsy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/docsy.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc",
  "main": "dist/index.js",
  "module": "dist/docsy.esm.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "lint": "tsdx lint src",
