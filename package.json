{
  "name": "docsy",
  "version": "1.0.0-17",
  "description": "A syntax for Component based documents",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/docsy#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/docsy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/docsy.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/docsy.es.js",
      "require": "./dist/docsy.cjs.js"
    }
  },
  "main": "./dist/docsy.cjs.js",
  "module": "./dist/docsy.es.js",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && vite build && yarn build:types && denoify",
    "build:types": "tsc -p ./tsconfig.build.json",
    "example:run": "vite serve example",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "test": "yarn lint && jest --coverage",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "denoify": "^0.10.5",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.3.1",
    "jest-ts-tsx-resolver": "^1.0.0",
    "np": "^7.5.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4",
    "vite": "^2.6.13"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
